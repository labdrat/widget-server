{
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.7",
    "@flydotio/node-demo": "^0.2.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "@types/pg": "^8.11.6",
    "tsx": "^4.16.0",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.5.0",
    "express": "^4.19.2",
    "pg": "^8.12.0"
  },
  "scripts": {
    "build": "tsc",
    "dev-up": "docker compose -f dev-postgres-compose.yml --env-file .env.development up -d",
    "dev-down": "docker compose -f dev-postgres-compose.yml --env-file .env.development down",
    "start": "node build/server.js",
    "dev": "dotenvx run -f .env.development -- tsx src/server.ts"
  },
  "type": "module"
}
